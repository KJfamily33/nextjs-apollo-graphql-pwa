    "localforage": "1.5.0",
    "next": "beta",
    "next-routes": "^1.4.2",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "styled-components": "2.1.0"
  },
  "description": "React NextJS Apollo GraphQL Boilerplate",
  "author": "Gufran Mirza <gufranmirza1@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
